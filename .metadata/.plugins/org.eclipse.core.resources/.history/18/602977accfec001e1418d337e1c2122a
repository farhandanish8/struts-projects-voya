<struts>
	<package name="studentapp" extends="struts-default">
		<action name="studform" class="com.struts.action.StudentAction" method="showStudForm">
			<result name="none">/WEB-INF/content/studform.jsp</result>
		</action>
		
		<action name="register" class="com.struts.action.StudentAction" method="execute">
			<result name="success">/WEB-INF/content/student-success.jsp</result>
			<result name="error">/WEB-INF/content/studform.jsp</result>
			<result name="input">/WEB-INF/content/studform.jsp</result>
		</action>
	</package>
</struts>